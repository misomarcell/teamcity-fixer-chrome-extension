{
  "name": "TeamCity Fixer",
  "version": "0.0.1",
  "icons": { "48": "icon48.png" },
  "manifest_version": 2,
  "description": "Fix all TeamCity failed builds with just one click!",
  "homepage_url": "http://github.com/misomarcell",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Fix TC!"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}